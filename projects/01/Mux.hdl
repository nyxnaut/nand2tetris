/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    
	Not(in=sel, out=nSel);
	And(a=a, b=nSel, out=aSel);
	And(a=b, b=sel, out=bSel);
	Or(a=aSel, b=bSel, out=out);
}
